{
  "framework": "nextjs",
  "regions": ["iad1"],
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1",
      "NODE_ENV": "production",
      "NODE_OPTIONS": "--max-old-space-size=4096 --max-old-space-size=4096",
      "DISABLE_ESLINT_PLUGIN": "true",
      "SKIP_ENV_VALIDATION": "true"
    }
  },
  "functions": {
    "app/**/*.{js,ts,jsx,tsx}": {
      "maxDuration": 10,
      "memory": 1024
    }
  },
  "outputDirectory": ".next"
}